{
  "$schema": "https://json.schemastore.org/eslintrc.json",
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["google", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "valid-jsdoc": "warn",
    "max-len": [
      "error",
      {
        "code": 90
      }
    ],
    "require-jsdoc": "error",
    "callback-return": "error",
    "use-isnan": "error"
  }
}
